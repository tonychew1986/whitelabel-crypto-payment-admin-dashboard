.breadcrumb
  background-color #DDD
  padding 8px 30px

  .breadcrumbLevel
    float left
    padding 0px 5px
    text-transform uppercase
    font-size 12px
    font-weight bold
    color #666

    a
      text-decoration underline

  .breadcrumbLevelArrow
    float left
    padding 0px 5px

    img
      width 14px
